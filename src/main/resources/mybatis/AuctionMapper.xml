<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.gamewin.weixin.mybatis.AuctionMybatisDao">
 
 	<select id="getMyAuctionAllList" parameterType="com.gamewin.weixin.model.QueryAuctionDto" resultType="com.gamewin.weixin.entity.Auction">
		SELECT u.*  FROM wx_auction u  WHERE u.isdelete=0 
		 
	</select>
	 
 
</mapper> 
